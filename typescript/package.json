{
  "name": "toolbox",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx run-many --target=serve --all --parallel",
    "start:nestjs-graphql-prisma": "nx run nestjs-graphql-prisma:serve",
    "start:nextjs-urql": "nx run nextjs-urql:serve",
    "start:nextjs-algolia": "nx run nextjs-algolia:serve",
    "generate": "nx run-many --target=prepare --projects=nestjs-graphql-prisma,nextjs-urql --parallel",
    "generate:nestjs-graphql-prisma": "nx run nestjs-graphql-prisma:prepare",
    "generate:nextjs-urql": "nx run nextjs-urql:prepare",
    "build": "yarn generate && nx run-many --target=build --all",
    "lint": "nx workspace-lint && nx run-many --target=lint --all",
    "format": "nx format:write"
  },
  "private": true,
