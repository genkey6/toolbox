  "license": "MIT",
  "scripts": {
    "start": "nx run-many --target=serve --all --parallel",
    "start:apollo-supergraph-gateway": "nx run apollo-supergraph-gateway:serve",
    "start:nestjs-apollo-subgraph": "nx run nestjs-apollo-subgraph:serve",
    "start:nestjs-graphql-prisma": "nx run nestjs-graphql-prisma:serve",
    "start:nextjs-algolia": "nx run nextjs-algolia:serve",
